\begin{UseCase}{CU 11}{Gestionar entidades}
	{
		Este caso de uso permite al analista visualizar los registros de las entidades del sistema. También permite 
		al actor acceder a las operaciones de registro, modificación y eliminación de una entidad.
	}
	
	\UCitem{Actor}{\cdtRef{actor:liderAnalisis}{Líder de análisis}, \cdtRef{actor:analista}{Analista}}
	\UCitem{Propósito}{
		Gestionar las entidades registradas en la herramienta.
	}
	\UCitem{Entradas}{
		Ninguna
	}
	\UCitem{Salidas}{
		\begin{UClist}
			\UCli \cdtRef{Modulo:Clave}{Clave del módulo}: \ioObtener.
			\UCli \cdtRef{Modulo:Nombre}{Nombre del módulo}: \ioObtener.
			\UCli \cdtRef{Modulo:Descripcion}{Descripción del módulo}: \ioObtener.
			\UCli \cdtRef{Entidad}{Entidad}: \ioTabla{\cdtRef{Elemento:Clave}{Clave}, \cdtRef{Elemento:Numero}{Número} y \cdtRef{Elemento:Nombre}{Nombre}}{de las entidades}.
		\end{UClist}
	}
	
	\UCitem{Mensajes}{
		\begin{UClist}
			\UCli \cdtIdRef{MSG2}{No existe información}: Se muestra en la pantalla \cdtIdRef{IU 5}{Gestionar casos de uso} cuando no existen casos de uso registrados.
		\end{UClist}
	}

	\UCitem{Precondiciones}{
		Ninguna
	}
	
	\UCitem{Postcondiciones}{
		\begin{UClist}
			\UCli Se podrá solicitar el registro de una entidad por medio del caso de uso \cdtIdRef{CU 11.1}{Registrar entidad}.
			\UCli Se podrá solicitar la modificación de una entidad por medio del caso de uso \cdtIdRef{CU 11.2}{Modificar entidad}.
			\UCli Se podrá solicitar la eliminación de una entidad por medio del caso de uso \cdtIdRef{CU 11.3}{Eliminar entidad}.
		\end{UClist}
	}

	\UCitem{Errores}{
		Ninguno
	}

	\UCitem{Tipo}{
		Primario
	}
\end{UseCase}
%-------------------------------------------------------%trayectoria Principal-----------------------------------------------
 \begin{UCtrayectoria}
    \UCpaso[\UCactor] Solicita gestionar los casos de uso presionando el botón \btnCU de la pantalla \cdtIdRef{IU 3}{Gestionar módulos}.
    \UCpaso[\UCsist] Busca la información de los casos de uso en cualquier estado. \refTray{A}
    \UCpaso[\UCsist] Muestra la información de los casos de uso en la pantalla \cdtIdRef{IU 5}{Gestionar casos de uso} y las operaciones 
    disponibles de acuerdo a la regla de negocio \cdtIdRef{RN-S9}{Operaciones disponibles de casos de uso}. 
    \UCpaso[\UCactor] Gestiona los casos de uso a través de los botones: \btnRevisar, \cdtButton{Registrar}, \btnConsulta, \btnEditar y \btnEliminar. \label{cu5:gestionaCU}
 \end{UCtrayectoria}
 
 \begin{UCtrayectoriaA}[Fin del caso de uso]{A}{No existen registros de casos de uso.}
    \UCpaso[\UCsist] Muestra el mensaje \cdtIdRef{MSG2}{No existe información} en pantalla \cdtIdRef{IU 5}{Gestionar casos de uso} 
    para indicar que no hay registros de casos de uso para mostrar.
 \end{UCtrayectoriaA}
 

